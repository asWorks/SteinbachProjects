<Window x:Class="ProjektDB.Reports.RepViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RepViewer" Height="650" Width="791"
        xmlns:local="clr-namespace:ProjektDB"
        Loaded="Window_Loaded" AllowsTransparency="False" Closing="Window_Closing">
    <Grid x:Name="GridDisplay" Height="605">
        <Grid.RowDefinitions>
            <RowDefinition Height="577*" />
            <RowDefinition Height="34*" />
        </Grid.RowDefinitions>
        <Button Content="PDF" Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="12,10,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Word" Height="23" HorizontalAlignment="Left" Margin="115,10,0,0" Name="button2" VerticalAlignment="Top" Width="75" Grid.Row="1" Click="button2_Click" />
    </Grid>
</Window>
